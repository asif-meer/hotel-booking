.float-left
  = render 'home/admin_links'
.float-right
  .container_16
    %h3 Users
    .block-border
      .block-content

        %table.roomlist{:cellspacing => 0}
          - @users.each do |user|
            %tr
              %th{:scope => 'row'}= user.email
              %td
                /= service.name
                /= link_to 'Show', user
                = link_to 'Edit', edit_user_path(user)
                = link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete
      %br
      =link_to 'New User', new_user_path