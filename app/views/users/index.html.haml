.float-left
  = render 'home/admin_links'
.float-right
  .container_16
    %h3 Users
    .block-border
      .block-content
        =link_to 'New User', new_user_path, :class => "links_btn"
        %br
        %br
        %table.roomlist{:cellspacing => 0}
          - @users.each do |user|
            %tr
              %th{:scope => 'row'}= user.email
              %td
                /= service.name
                /= link_to 'Show', user
                = link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete, :class => "links_btn right del_btn_margin"
                = link_to 'Edit', edit_user_path(user), :class => "links_btn right"
      %br