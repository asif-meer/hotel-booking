.float-left
  = render 'home/admin_links'
.float-right
  .container_16
    %h3 Services
    .block-border
      .block-content
        =link_to 'New Service', new_service_path, :class => "links_btn"
        %br
        %br
        %table.roomlist{:cellspacing => 0}
          - @services.each do |service|
            %tr
              %th{:scope => 'row'}= service.name
              %td
                /= service.name
                /= link_to 'Show', room  
                = link_to 'Delete', service, :confirm => 'Are you sure?', :method => :delete, :class=>"links_btn right del_btn_margin"
                = link_to 'Edit', edit_service_path(service), :class=>"links_btn right"
      %br